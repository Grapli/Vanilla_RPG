<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Vanilla_RPG || Travel</title>
    <!-- @include head.kit -->
    <link rel="stylesheet" href="/dist/css/app.min.css">
</head>

<body class="travel">
    <div class="game-container">
        <h1 class="game-title">_Vanilla_RPG_</h1>

        <div class="table">

            <div class="character">
                <div class="character__info">
                    <p class="character__info-item">Health: <span class="hp-stat"></span></p>
                    <p class="character__info-item">Attack: <span class="atk-stat"></span></p>
                    <p class="character__info-item">Defense: <span class="def-stat"></span></p>
                    <p class="character__info-item">Level: <span class="lvl-stat"></span></p>
                    <p class="character__info-item">Need Exp: <span class="xp-stat"></span></p>
                </div>
            </div>

            <div class="map">
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
                <div class="map-tile"></div>
            </div>

            <div class="actions__travel">
                <h2 class="actions__travel__title">Locations</h2>
                <div class="actions__travel__box">
                    <button class="actions__travel__item">Hyperion</button>
                    <button class="actions__travel__item">Nomedia</button>
                    <button class="actions__travel__item">Genao</button>
                </div>
            </div>

            <div class="actions__menu">
                <h2 class="actions__menu__title">Menu</h2>
                <div class="actions__menu__box">
                    <button class="actions__menu__item">Home</button>
                    <button class="actions__menu__item">Inventory</button>
                    <button class="actions__menu__item">Quests</button>
                </div>
            </div>


        </div>
    </div>


    <script src="/dist/js/app.min.js"></script>
</body>

</html>